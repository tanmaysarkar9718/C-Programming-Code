                                                      /*Matrix Multiplication*/
#include <stdio.h>
int main()
{
    int row1,col1,row2,col2;
    printf("Enter the row of matrix a:\n");
    scanf("%d",&row1);
    printf("Enter the column of matrix a:\n");
    scanf("%d",&col1);
    printf("Enter the row of matrix b:\n");
    scanf("%d",&row2);
    printf("Enter the column of matrix b:\n");
    scanf("%d",&col2);
    int i,j,k,a[row1][col1],b[row2][col2],mul[row1][col2];
    if(col1==row2)
    {
    for(i=0;i<row1;i++)
    {
        for(j=0;j<col1;j++)
        {
            printf("Enter the value of a[%d][%d]\n",i+1,j+1);
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<row2;i++)
    {
        for(j=0;j<col2;j++)
        {
            printf("Enter the value of b[%d][%d]\n",i+1,j+1);
            scanf("%d",&b[i][j]);
        }
    }
    for(i=0;i<row1;i++)
    {
        for(j=0;j<col2;j++)
        {
            mul[i][j]=0;  
            for( k=0;k<row2 ;k++)    
         {
             mul[i][j]= mul[i][j] + a[i][k]*b[k][j];
         }
        }
    }
    printf("The Multiplication is:-");
    printf("\n");
    for(i=0;i<row1;i++)
    {
        for(j=0;j<col2;j++)
        {
            printf("%d ",mul[i][j]);
        }
        printf("\n");
    }
    }
    else
    {
        printf("Math Error!");
    }
    return 0;
}
