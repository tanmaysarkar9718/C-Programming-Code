                                               /*Length of a String*/
#include <stdio.h>
int main()
{
    char name[25];
    int i,count=0;
    printf("Enter your name:\n");
    gets(name);
    for( i=0;name[i]!='\0';i++)
    {
        count++;
    }
    printf("The length of the string is: %d",count);
    return 0;
}
